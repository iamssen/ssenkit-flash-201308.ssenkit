package ssen.common.date {

public class Microtimes {
	/** microtime 1000 */
	public static const SECOND:int=1000;

	/** microtime 1000 * 60 */
	public static const MINUTE:int=60000;

	/** microtime 1000 * 60 * 60 */
	public static const HOUR:int=3600000;

	/** microtime 1000 * 60 * 60 * 24 */
	public static const DAY:int=86400000;

	/** microtime 1000 * 60 * 60 * 24 * 7 */
	public static const WEEK:int=604800000;
}
}
